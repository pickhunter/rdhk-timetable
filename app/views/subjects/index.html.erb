<p id="notice"><%= notice %></p>

<h1>Listing subjects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td> <%= subject.name %></td>
        <td> <%= subject.semester/2 + 1 %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

